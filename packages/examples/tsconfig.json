{
  "compilerOptions": {
    "target": "es2015",
    "moduleResolution": "node",
    "lib": ["es6", "dom", "esnext.asynciterable"],
    "experimentalDecorators": true,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "lib",
    "plugins": [
      {
        "transform": "@crayonzx/mobx-utils/lib/ts-transform-async-to-flow-generator",
        "type": "config"
      }
    ]
  },
  "exclude": ["node_modules"]
}
